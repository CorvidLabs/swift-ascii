{
  "name": "@corvidlabs/ascii-pixel-art",
  "version": "0.1.0",
  "description": "Convert ASCII art to pixel art SVG - compiled from Swift to WebAssembly",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:wasm": "swift run carton bundle --product ascii-pixel-wasm",
    "build": "npm run build:wasm && npm run build:wrapper",
    "build:wrapper": "node scripts/build-wrapper.js",
    "dev": "swift run carton dev --product ascii-pixel-wasm",
    "test": "node test/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CorvidLabs/swift-ascii.git"
  },
  "keywords": [
    "ascii",
    "pixel-art",
    "svg",
    "wasm",
    "webassembly",
    "swift"
  ],
  "author": "Zach Eriksen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CorvidLabs/swift-ascii/issues"
  },
  "homepage": "https://github.com/CorvidLabs/swift-ascii#readme",
  "dependencies": {
    "javascript-kit-swift": "^0.21.0"
  },
  "devDependencies": {}
}
